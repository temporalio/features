  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "eslint-plugin-tsdoc": "^0.2.14",
    "prettier": "^2.3.2",
    "ts-node": "^10.4.0",
